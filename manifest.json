{
    "manifest_version": 3,
    "name": "To Do List",
    "version": "1.0",
    "description": "To Do Extension with a default side panel.",
    "icons": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "permissions": ["sidePanel"]
  }